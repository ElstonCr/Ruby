<div id="<%= dom_id friend %>">
  <p>
    <strong>First name:</strong>
    <%= friend.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= friend.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= friend.Email %>
  </p>

  <p>
    <strong>Contact number:</strong>
    <%= friend.contact_number %>
  </p>

  <p>
    <strong>Linkedin:</strong>
    <%= friend.linkedIn %>
  </p>

</div>
